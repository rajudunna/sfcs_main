		        showHide_enable_tick_all: true 

               /* showHide_cols_tick_to_hide: false,  
                btn_showHide_cols_target_id: 'spnColMng',  
                showHide_cols_at_start: [],  
                btn_showHide_cols_html: '<button>Columns manager ▼</button>',  
